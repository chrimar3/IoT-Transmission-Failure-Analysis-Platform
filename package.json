{
  "name": "cu-bems-iot-platform",
  "version": "1.0.0",
  "description": "Enterprise-grade IoT analytics platform for building energy management systems. Analyzes 124.9M sensor records to identify transmission failures, energy anomalies, and maintenance opportunities worth $273,500 in annual savings.",
  "private": false,
  "author": {
    "name": "Christopher Marroquin",
    "email": "contact@github.com",
    "url": "https://github.com/chrimar3"
  },
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/chrimar3/IoT-Transmission-Failure-Analysis-Platform.git"
  },
  "bugs": {
    "url": "https://github.com/chrimar3/IoT-Transmission-Failure-Analysis-Platform/issues"
  },
  "homepage": "https://github.com/chrimar3/IoT-Transmission-Failure-Analysis-Platform#readme",
  "keywords": [
    "iot",
    "analytics",
    "building-management",
    "energy-management",
    "bems",
    "sensor-data",
    "data-analytics",
    "nextjs",
    "typescript",
    "react",
    "supabase",
    "postgresql",
    "stripe",
    "subscription",
    "saas",
    "enterprise",
    "real-time",
    "dashboard",
    "visualization",
    "predictive-maintenance"
  ],
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "typecheck": "tsc --noEmit",
    "test": "jest",
    "test:unit": "jest --testPathPattern=__tests__ --passWithNoTests",
    "test:integration": "jest __tests__/integration",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:ci": "jest --ci --coverage --watchAll=false",
    "db:test": "node tools/deployment/test-supabase-connection.js",
    "db:setup": "node tools/deployment/setup-supabase-schema.js",
    "db:status": "node tools/deployment/check-supabase-status.js",
    "db:migrate": "psql $DATABASE_URL -f config/database/001-core-schema.sql -f config/database/002-materialized-views.sql -f config/database/003-rls-policies.sql -f config/database/004-nextauth-schema.sql -f config/database/005-stripe-subscription-schema.sql",
    "env:check": "node tools/deployment/validate-env.js",
    "env:validate-production": "node scripts/validate-production-env.js",
    "env:template": "node scripts/validate-production-env.js template",
    "screenshots": "node tools/deployment/generate-screenshots.js",
    "docs:images": "npm run screenshots",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "clean": "rm -rf .next dist coverage",
    "validate": "npm run typecheck && npm run lint && npm run test:ci",
    "validate:production": "tsx scripts/validate-production-readiness.ts",
    "install:bmad": "node tools/deployment/install-bmad.js",
    "bmad:refresh": "bmad-method install -f -i codex",
    "bmad:list": "bmad-method list:agents",
    "bmad:validate": "bmad-method validate",
    "domain:setup": "node scripts/domain/setup-domain.js",
    "domain:verify": "curl -f https://cu-bems.vercel.app/api/health || echo 'Domain verification failed'",
    "qa:gate": "./scripts/quality-monitoring.sh",
    "qa:monitor": "./scripts/quality-monitoring.sh",
    "qa:dashboard": "./scripts/quality-monitoring.sh && open reports/quality/quality-dashboard-*.html",
    "security:scan": "npm audit --audit-level high",
    "perf:baseline": "echo 'Performance baseline test - implement with your preferred tool'",
    "health:validate": "curl -f http://localhost:3000/api/health || echo 'Health check failed'",
    "monitoring:verify": "echo 'Monitoring verification - implement with your observability stack'"
  },
  "dependencies": {
    "@hookform/resolvers": "^3.3.2",
    "@next-auth/supabase-adapter": "^0.2.1",
    "@prisma/client": "^6.16.2",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-progress": "^1.1.7",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.7",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.3",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@react-pdf/renderer": "^4.3.1",
    "@supabase/auth-helpers-nextjs": "^0.8.7",
    "@supabase/supabase-js": "^2.39.0",
    "@types/pdfkit": "^0.17.3",
    "chart.js": "^4.5.0",
    "chartjs-adapter-date-fns": "^3.0.0",
    "class-variance-authority": "^0.7.1",
    "csv-parser": "^3.2.0",
    "date-fns": "^3.0.6",
    "dotenv": "^17.2.2",
    "exceljs": "^4.4.0",
    "gray-matter": "^4.0.3",
    "isomorphic-dompurify": "^2.28.0",
    "lucide-react": "^0.544.0",
    "next": "14.2.15",
    "next-auth": "^4.24.5",
    "pdf-lib": "^1.17.1",
    "pdfkit": "^0.17.2",
    "prisma": "^6.16.2",
    "puppeteer": "^24.20.0",
    "react": "^18.2.0",
    "react-chartjs-2": "^5.3.0",
    "react-dnd": "^16.0.1",
    "react-dnd-html5-backend": "^16.0.1",
    "react-dom": "^18.2.0",
    "react-hook-form": "^7.48.2",
    "recharts": "^2.10.1",
    "stripe": "^14.11.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-react": "^5.29.1",
    "tailwind-merge": "^3.3.1",
    "xlsx": "^0.18.5",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@babel/preset-typescript": "^7.27.1",
    "@faker-js/faker": "^10.0.0",
    "@testing-library/jest-dom": "^6.1.6",
    "@testing-library/react": "^14.1.2",
    "@testing-library/user-event": "^14.5.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.45",
    "@types/react-dom": "^18.2.18",
    "@types/swagger-jsdoc": "^6.0.4",
    "@types/swagger-ui-react": "^5.18.0",
    "autoprefixer": "^10.4.16",
    "bmad-method": "^4.43.1",
    "eslint": "^8.56.0",
    "eslint-config-next": "14.2.15",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^29.7.0",
    "jest-environment-jsdom": "^29.7.0",
    "jest-mock-extended": "^3.0.5",
    "postcss": "^8.4.32",
    "tailwindcss": "^3.4.0",
    "ts-jest": "^29.4.4",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
